{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "uploads/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/courses",
      "dest": "/server.js"
    },
    {
      "src": "/auth/login",
      "dest": "/server.js"
    },
    {
      "src": "/accounts",
      "dest": "/server.js"
    },
    {
      "src": "/accounts/check/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/courses/requests",
      "dest": "/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Mainpage.html"
    }
  ]
}
